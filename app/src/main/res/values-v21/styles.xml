<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#999</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <color name="background">#2F3C50</color>

    <style name="stylee" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@color/background</item>
        <item name="android:statusBarColor">@color/background</item>
    </style>

    <style name="portalHome" parent="Theme.AppCompat.Light"></style>

    <style name="description" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/background</item>
        <item name="android:statusBarColor">#FF181E29</item>
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>

    <style name="textLabel" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/background</item>
        <item name="colorPrimary">@color/background</item>
        <item name="android:statusBarColor">#FF181E29</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>

    <style name="showMap" parent="Theme.AppCompat.Light">
        <item name="android:statusBarColor">#FF181E29</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="colorPrimary">@color/background</item>
    </style>

    <style name="textStyle"  parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">#FF181E29</item>

    </style>
</resources>